    <div class="report-form">
      <h2>Report an Issue</h2>
      <form action="/" method="post">
        <label for="issue-type">Issue Type:</label>
        <select name="issue-type" id="issue-type">
          <option value="incorrect-item">Incorrect Item</option>
          <option value="damaged-item">Damaged Item</option>
          <option value="missing-item">Missing Item</option>
          <option value="late-delivery">Late Delivery</option>
          <option value="payment-error">Payment Error</option>
        </select>
  
        <label for="order-id">Order ID:</label>
        <input type="text" name="order-id" id="order-id" required>
  
        <label for="description">Description:</label>
        <textarea name="description" id="description" rows="5" required></textarea>
  
        <label for="attachments">Attachments:</label>
        <input type="file" name="attachments" id="attachments">
  
        <label for="contact-email">Contact Email:</label>
        <input type="email" name="contact-email" id="contact-email">
  
        <label for="contact-phone">Contact Phone:</label>
        <input type="tel" name="contact-phone" id="contact-phone">
  
        <label for="expected-resolution">Expected Resolution:</label>
        <textarea name="expected-resolution" id="expected-resolution" rows="3"></textarea>
  
        <button type="submit">Submit Report</button>
      </form>
    </div>
